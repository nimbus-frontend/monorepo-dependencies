{
  "name": "nimbus-frontend",
  "version": "0.0.0-DO-NOT-TOUCH",
  "scripts": {
    "prepare": "husky install",
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2015 browser module main",
    "install-new": "npm run install-legacy",
    "install-dep": "npm run install-legacy -S -E",
    "install-dev": "npm run install-legacy -D -E",
    "install-fast": "npm run install-legacy --prefer-offline",
    "install-strict": "npm ci --quiet --prefer-offline --cache=./tmp/npm",
    "install-clean": "npm i --quiet --cache=./tmp/npm",
    "install-force": "npm i --force --quiet --cache=./tmp/npm",
    "install-legacy": "npm i --legacy-peer-deps --quiet --cache=./tmp/npm",
    "ng": "nx",
    "nx": "nx",
    "affected": "nx affected",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:graph": "nx affected:dep-graph",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "lint-staged": "nx format:write && nx run-many --all --target=lint --fix",
    "generator": "nx workspace-generator",
    "dep-graph": "nx dep-graph"
  },
